<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Unique invitations, for unique events">
        <title>{% block title %}ElleOL Designs{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets '@bootstrap' 'bundles/elleolfe/css/*.css' output="css/core.css" filter="cssrewrite" %}
                <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body>
        <div class="dots z1"></div>
        <div class="dots z2"></div>
        {% block body %}
            {% block navbar %}
                {% include 'ElleOLFEBundle::navbar.html.twig' %}
            {% endblock %}
            <div id="main-container" class="container">
                {% block content %}{% endblock %}
            </div>
        {% endblock %}
        {% block javascripts %}
            {% javascripts '@corejs' output="js/core.js" %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
    <script src="//localhost:35729/livereload.js"></script>

</html>
